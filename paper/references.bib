  title = {Differences in outcomes following an intensive upper-limb
  rehabilitation program for patients with common central nervous system-acting
  drug prescriptions},
  author = {Johnstone, A and Brander, F and Kelly, K and Bestmann, S and Ward, N},
  journaltitle = {International journal of stroke: official journal of the
