  note = {Export Date: 8 June 2022 Correspondence Address: Kritzler, S.;
  Department of Psychology, Germany; email: Sarah.Kritzler
@ARTICLE{Bogdanov2021-xh,
  title = {Acute Psychosocial Stress Increases Cognitive-Effort Avoidance},
  author = {Bogdanov, Mario and Nitschke, Jonas P and LoParco, Sophia and Bartz,
  Jennifer A and Otto, A Ross},
