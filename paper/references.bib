@ARTICLE{Berggren2016-vp,
  title = {Does contralateral delay activity reflect working memory storage or
  the current focus of spatial attention within visual working memory?},
  author = {Berggren, Nick and Eimer, Martin},
  journaltitle = {Journal of cognitive neuroscience},
