  pages = {78-86},
  date = {2015},
  doi = {10.1016/j.neuroimage.2015.01.059},
  issn = {1053-8119},
  abstract = {Maintaining information in visual working memory is reliably
  indexed by the contralateral delay activity (CDA) - a sustained modulation of
