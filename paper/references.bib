  publisher = {Oxford University Press},
  location = {Bunge, Silvia A.: Massachusetts Inst of Technology, Dept of Brain
  and Cognitive Sciences, NE-20, Cambridge, MA, US, 02139,
  sbunge
@psych.stanfordalumni.org},
  volume = {124},
  issue = {10},
  pages = {2074-2086},
  date = {2001-10-01},
  doi = {10.1093/brain/124.10.2074},
  issn = {0006-8950,1460-2156},
  abstract = {Goal-directed behaviour depends on keeping relevant information in
  mind (working memory) and irrelevant information out of mind (behavioural
  inhibition or interference resolution). Prefrontal cortex is essential for
  working memory and for interference resolution, but it is unknown whether
  these abilities are mediated by common or distinct prefrontal regions.
  Functional MRI was used to identify brain regions activated by separate
  manipulations of working memory load and interference within a single task
  (the Sternberg item recognition paradigm). Both load and interference
  manipulations were associated with performance decrements. Ss (aged 18-40 yrs)
  were unaware of the interference manipulation. There was a high degree of
  overlap between the regions activated by load and interference. Critically, no
  region was activated exclusively by interference. Several regions within this
  network exhibited a brain-behaviour correlation across Ss for the load or
  interference manipulation. Within the circuitry engaged by this task, some
  regions are more critically involved in the resolution of interference whereas
  others are more involved in the resolution of an increase in load. (PsycINFO
  Database Record (c) 2016 APA, all rights reserved)},
  url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=psyc3&NEWS=N&AN=2001-11828-010},
  keywords = {*Interference (Learning); *Neurophysiology; *Prefrontal Cortex;
  *Psychophysiology; *Short Term Memory; Behavioral Inhibition;WM}
}

@ARTICLE{Ramachandran1997-tg,
  title = {Three laws of qualia: What neurology tells us about the biological
  functions of consciousness},
  author = {Ramachandran, V S and Hirstein, William},
  journaltitle = {Journal of Consciousness Studies},
  publisher = {Imprint Academic},
  location = {US},
  volume = {4},
  issue = {5-6},
  pages = {429-457},
  date = {1997},
  issn = {1355-8250},
  abstract = {Argues that an investigation into neurological syndromes in which
  consciousness seems to malfunction (e.g., temporal lobe epilepsy) reveals that
  qualia are different from other brain states in that they possess 3 functional
  characteristics, which the authors state in the form of "3 laws of qualia"
  based on a loose analogy with Newton's 3 laws of classical mechanics. First,
  they are irrevocable: the individual cannot simply decide to start seeing the
  sunset as green, or feel pain as if it were an itch; second, qualia do not
  always produce the same behaviour: given a set of qualia, individuals can
  choose from a potentially infinite set of possible behaviours to execute; and
  third, qualia endure in short-term memory, as opposed to non-conscious brain
  states involved in the on-line guidance of behaviour in real time. The authors
  suggest that qualia have evolved these and other attributes (e.g. they are
  "filled in") because of their role in facilitating non-automatic,
  decision-based action. They also suggest that the apparent epistemic barrier
  to knowing what qualia another person is experiencing can be overcome simply
  by using a "bridge" of neurons; and they offer a hypothesis about the relation
  between qualia and one's sense of self. (PsycINFO Database Record (c) 2017
  APA, all rights reserved)},
  series = {Special Section: Models of the Self.},
  url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=psyc3&NEWS=N&AN=1998-00856-002},
  keywords = {*Consciousness States; *Neurophysiology;
  *Self-Perception;Consciousness}
}

