  title = {‘Time-shrinking perception’ in the visual system: a psychophysical
  and high-density ERP study},
  author = {Nagaike, Atsushi and Mitsudo, Takako and Nakajima, Yoshitaka and
  Ogata, Katsuya and Yamasaki, Takao and Goto, Yoshinobu and Tobimatsu, Shozo},
  journaltitle = {Experimental brain research. Experimentelle Hirnforschung.
