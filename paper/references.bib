  title = {The impact of early visual cortex transcranial magnetic stimulation
  on visual working memory precision and guess rate},
  author = {Rademaker, Rosanne L and Van De Ven, Vincent G and Tong, Frank and
  Sack, Alexander T},
  journaltitle = {PloS one},
