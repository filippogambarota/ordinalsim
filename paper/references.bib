  keywords = {*Implicit Learning; *Insight; *Problem Solving; *Short Term
  Memory; *Thinking;WM}
}

@ARTICLE{Raggett2014-ps,
  title = {Review of The conscious brain: How attention engenders experience},
  author = {Raggett, Simon},
  editor = {Carhart-Harris Fries, Gaillard, Goldberg, Holz, Kosiol, Kreiman,
  Malach, Malach, Mangun, Quiroga, Quiroga, Quiroga, Rolls, Rolls, Sauseng,
  Shiradi, Summerfield, Taylor, Wang, Wyart, Zald, Ogmen, Chang},
  journaltitle = {Journal of Consciousness Studies},
  publisher = {Imprint Academic},
  location = {US},
  volume = {21},
  issue = {1-2},
  pages = {215-221},
  date = {2014},
  issn = {1355-8250},
  abstract = {Reviews the book, The Conscious Brain: How Attention Engenders
  Experience by Jesse Prinz (2012). In this book, Prinz develops his AIR theory
  of consciousness. He envisages two stages that are necessary to establish a
  theory of consciousness. Firstly, there must be a particular process by which
  brain states become conscious. Secondly, we need to know where in the brain
  this occurs. Neural processing as such has no need of consciousness. There is
  now abundant evidence for unconscious processing and perception of stimuli.
  Something else is needed to push processing into consciousness. Prinz views
  the term qualia as interchangeable with phenomenal states, experiences, and
  feelings. Qualia are suggested to be mind dependent properties, in the sense
  that there is no blue out there, but only blue created in the brain. Neurons
  that code for blue must have some distinctive property or process that sets
  them apart from, for instance, neurons that code for red, although there does
  not appear to be a detectable physical distinction between the neurons
  involved. There is some evidence to suggest that different color experiences
  correlate to the temporal patterns of neuronal firing. (PsycINFO Database
  Record (c) 2017 APA, all rights reserved)},
  url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=psyc11&NEWS=N&AN=2014-08512-013},
  keywords = {*Attention; *Brain; *Consciousness States; *Emotions; *Short Term
  Memory;Consciousness}
}

