  title = {Working memory requires conscious processes, not vice versa: A Global
  Workspace account},
  author = {Baars, B J},
  editor = {Anderson Baars, Baars, Baars, Baars, Baars, Baars, Baars, Baars,
  Baars, Baars, Baars, Baddeley, Baddeley, Bisiach, Bogen, Chase, Courtney,
