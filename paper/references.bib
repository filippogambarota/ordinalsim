}

@ARTICLE{Sander2011-qb,
  title = {Contralateral delay activity reveals life-span age differences in
  top-down modulation of working memory contents},
  author = {Sander, M C and Werkle-Bergner, M and Lindenberger, U},
  journaltitle = {Cerebral cortex},
