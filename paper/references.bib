@ARTICLE{Voracek2019-kz,
  title = {Which Data to Meta-Analyze, and How?: A Specification-Curve and
  Multiverse-Analysis Approach to Meta-Analysis},
  author = {Voracek, M and Kossmeier, M and Tran, U S},
  journaltitle = {Swiss journal of psychology: official publication of the Swiss
  Psychological Society Schweizerische Zeitschrift fur Psychologie = Revue
  suisse de psychologie},
  publisher = {Hogrefe Verlag GmbH \& Co. KG},
  volume = {227},
