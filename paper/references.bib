  author = {Tong, F and Nakayama, K and Vaughan, J T and Kanwisher, N},
  journaltitle = {Neuron},
  publisher = {Elsevier},
  volume = {21},
  issue = {4},
  pages = {753-759},
  date = {1998-10},
  doi = {10.1016/s0896-6273(00)80592-9},
  pmid = {9808462},
  issn = {0896-6273},
  abstract = {We used functional magnetic resonance imaging (fMRI) to monitor
  stimulus-selective responses of the human fusiform face area (FFA) and
  parahippocampal place area (PPA) during binocular rivalry in which a face and
  a house stimulus were presented to different eyes. Though retinal stimulation
  remained constant, subjects perceived changes from house to face that were
  accompanied by increasing FFA and decreasing PPA activity; perceived changes
  from face to house led to the opposite pattern of responses. These responses
  during rivalry were equal in magnitude to those evoked by nonrivalrous
  stimulus alternation, suggesting that activity in the FFA and PPA reflects the
  perceived rather than the retinal stimulus, and that neural competition during
  binocular rivalry has been resolved by these stages of visual processing.},
  url = {http://dx.doi.org/10.1016/s0896-6273(00)80592-9},
  language = {en}
}

@ARTICLE{Homa2001-ri,
  title = {Recognition of facial prototypes: The importance of categorical
  structure and degree of learning},
  author = {Homa, D and Smith, C and Macak, C and Johovich, J and Osorio, D},
  journaltitle = {Journal of memory and language},
  volume = {44},
  issue = {3},
  pages = {443-474},
  date = {2001},
  doi = {10.1006/jmla.2000.2723},
  issn = {0749-596X},
  abstract = {The importance of categorical structure in the recognition and
  classification of realistic face prototypes, following a variable number of
  learning trials, was investigated in four experiments. Subjects either
  classified or observed well-defined (Experiments 1-3) or ill-defined
  (Experiment 4) faces belonging to three categories for one or nine trial
  blocks, followed by a transfer test. Recognition performance following
  classification (Experiment 1) and observational (Experiments 2 and 3) training
  was virtually the same: Oldness judgments were highest for the category
  prototypes and old instances and were least for new and random instances, with
  these tendencies increased following nine learning trials. When features were
  made ill defined (Experiment 4), oldness judgments decreased for the category
  prototype following nine learning trials. Multidimensional scaling (Experiment
  5) revealed that the category structure for the well-defined and ill-defined
  faces was globally similar, and therefore, the disparate recognition of the
  category prototype was not due to a radical reorganization of the stimulus
  space. These results suggest that false recognition of the category prototype
  is at least partially based on type of category structure, and that prior
  studies using false recognition of the category prototype as evidence of an
  earlier abstraction process are probably misguided. Rather, false recognition
  is more likely due to erroneous combinations of features, with category
  influences playing less of a role. © 2001 Academic Press.},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0035743272&doi=10.1006%2fjmla.2000.2723&partnerID=40&md5=9320416a17700ebe8e2fb0026a927115},
  note = {cited By 12},
  keywords = {Face Similarity},
  language = {English}
}

@ARTICLE{Peters2010-cd,
  title = {Assessing publication bias in meta-analyses in the presence of
  between-study heterogeneity},
  author = {Peters, Jaime L and Sutton, Alex J and Jones, David R and Abrams,
  Keith R and Rushton, Lesley and Moreno, Santiago G},
  journaltitle = {Journal of the Royal Statistical Society. Series A,},
  publisher = {Wiley},
  volume = {173},
  issue = {3},
  pages = {575-591},
  date = {2010-01-04},
  doi = {10.1111/j.1467-985x.2009.00629.x},
  issn = {0964-1998,1467-985X},
  abstract = {Summary.? Between-study heterogeneity and publication bias are
  common features of a meta-analysis that can be present simultaneously. When
  both are suspected, consideration must be made of each in the assessment of
  the other. We consider extended funnel plot tests for detecting publication
  bias, and selection modelling and trim-and-fill methods to adjust for
  publication bias in the presence of between-study heterogeneity. These methods
  are applied to two example data sets. Results indicate that ignoring
  between-study heterogeneity when assessing publication bias can be misleading,
  but that methods to test or adjust for publication bias in the presence of
  heterogeneity may not be powerful when the meta-analysis is not large. It is
  therefore unrealistic to expect to disentangle the effects of publication bias
  and heterogeneity reliably in all except the largest meta-analyses.},
  url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1467-985X.2009.00629.x},
  keywords = {MA Unconscious WM},
  language = {en}
}

